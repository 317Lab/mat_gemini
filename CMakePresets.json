{
  "version": 3,

"configurePresets": [
{
  "name": "default",
  "binaryDir": "${sourceDir}/build",
  "cacheVariables": {
    "package": false,
    "equil": false
  }
},
{
  "name": "regen-package", "inherits": "default",
  "cacheVariables": {
    "equil": true,
    "package": true
  }
}
],
"testPresets": [
{
  "name": "default",
  "configurePreset": "default",
  "output": {
    "outputOnFailure": true,
    "verbosity": "default"
  },
  "execution": {
    "noTestsAction": "error",
    "stopOnFailure": false
  }
},
{
  "name": "regen-package", "inherits": "default",
  "configurePreset": "regen-package",
  "description": "Regenerates .zst reference data, by disabling compare",
  "filter": {
    "exclude": {
      "label": "(compare|download)",
      "fixtures": {
        "any": "(compare_fxt|inputOK_fxt)"
        }
      }
  }
}
]
}
